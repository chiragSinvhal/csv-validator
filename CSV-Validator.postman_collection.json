            {
              "key": "file",
              "type": "file",
              "src": []
            }
          ]
        },
        "url": {
          "raw": "{{base_url}}/api/upload"
        }
      }
    },
    {
      "name": "Download File",
      "request": {
        "method": "GET",
        "url": {
          "raw": "{{base_url}}/api/download/{{job_id}}"
        }
      }
    }
  ]
